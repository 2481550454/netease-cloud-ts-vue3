{
  "name": "netease-cloud-demo-vue3-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build-run": "vue-tsc --noEmit && (为了异步代码分包这一段从build中移除)",
    "build": "vite build",
    "preview": "vite preview",
    "eslint:comment": "使用 ESLint 检查并自动修复 src 目录下所有扩展名为 .js 和 .vue 的文件",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier:comment": "自动格式化当前目录下的所有文件",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "prepare": "husky install",
    "commit": "git add . && node ./node_modules/cz-customizable/standalone.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@types/node": "^17.0.34",
    "@unocss/reset": "^0.33.5",
    "@vueuse/core": "^8.5.0",
    "axios": "^0.27.2",
    "element-plus": "^2.2.0",
    "pinia": "^2.0.14",
    "sass": "^1.51.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@iconify-json/ic": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.4",
    "unocss": "0.33.5",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9",
    "vue-global-api": "^0.4.1",
    "vue-tsc": "^0.34.15"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
}
